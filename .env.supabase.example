# Example .env file for Supabase migration
# Copy this to .env and fill in your actual values

# OCR.space API Key (Required)
OCRSPACE_API_KEY=your_ocr_key_here

# Supabase Configuration (NEW)
SUPABASE_URL=https://xxxxx.supabase.co
SUPABASE_KEY=eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9...

# Optional: Skip heavy model loading (for Railway free tier)
SKIP_MODEL_LOADING=false

# Optional: Port (Railway sets this automatically)
PORT=8080
